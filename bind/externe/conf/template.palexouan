$ORIGIN palexouan.

@ 				IN SOA 		palexouan. root.palexouan. (2 ; Serial
															604800 ; Refresh
															86400 ; Retry
															2419200 ; Expire
															604800 ) ; Negative Cache TTL
@ 				IN 	NS 		dns.palexouan.
@		 		IN 	A 		%address% 				; %address% sera remplacé par une adresse IP lors du démarrage de l'image
dns		 		IN 	A 		%address%
www		 		IN 	A 		%address%
